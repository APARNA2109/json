{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "iam:PassRole",
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "iam:PassedToService": [
            "lambda.amazonaws.com",
            "states.amazonaws.com",
            "emr-serverless.amazonaws.com",
            "sagemaker.amazonaws.com"
          ],
          "aws:ResourceAccount": "${aws:PrincipalAccount}"
        }
      }
    },
    {
      "Sid": "AllowCreateServiceLinkedRoleForApprovedServices",
      "Effect": "Allow",
      "Action": "iam:CreateServiceLinkedRole",
      "Resource": [
        "arn:aws:iam::*:role/aws-service-role/lambda.amazonaws.com/AWSServiceRoleForLambda",
        "arn:aws:iam::*:role/aws-service-role/states.amazonaws.com/AWSServiceRoleForStates",
        "arn:aws:iam::*:role/aws-service-role/sagemaker.amazonaws.com/AWSServiceRoleForAmazonSageMakerNotebooks"
      ],
      "Condition": {
        "StringEquals": {
          "iam:AWSServiceName": [
            "lambda.amazonaws.com",
            "states.amazonaws.com",
            "sagemaker.amazonaws.com"
          ]
        }
      }
    },
