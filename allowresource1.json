{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "iam:PassRole",
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "iam:PassedToService": [
            "lambda.amazonaws.com",
            "states.amazonaws.com",
            "emr-serverless.amazonaws.com",
            "sagemaker.amazonaws.com",
            "glue.amazonaws.com",
            "redshift.amazonaws.com"
          ]
        }
      }
    },
    {
      "Sid": "AllowCreateServiceLinkedRoleForApprovedServices",
      "Effect": "Allow",
      "Action": "iam:CreateServiceLinkedRole",
      "Resource": [
        "arn:aws:iam::*:role/aws-service-role/lambda.amazonaws.com/AWSServiceRoleForLambda",
        "arn:aws:iam::*:role/aws-service-role/states.amazonaws.com/AWSServiceRoleForStates",
        "arn:aws:iam::*:role/aws-service-role/sagemaker.amazonaws.com/AWSServiceRoleForAmazonSageMakerNotebooks"
      ],
      "Condition": {
        "StringEquals": {
          "iam:AWSServiceName": [
            "lambda.amazonaws.com",
            "states.amazonaws.com",
            "sagemaker.amazonaws.com"
          ]
        }
      }
    },
    {
      "Sid": "EC2IAMCloudServices",
      "Effect": "Allow",
      "Action": [
        "lambda:*",
        "ec2:AuthorizeSecurityGroupIngress",
        "ec2:DescribeInstanceAttribute",
        "ec2:GetInstanceUefiData",
        "ec2:ReplaceRouteTableAssociation",
        "ec2-instance-connect:*",
        "ec2:ModifyVolumeAttribute",
        "ec2:ModifyManagedPrefixList",
        "ec2:CreateKeyPair",
        "ec2:GetConsoleScreenshot",
        "ec2:GetConsoleOutput",
        "ec2:GetLaunchTemplateData",
        "ec2:AttachInternetGateway",
        "ec2:DeleteRouteTable",
        "ec2:ModifyInstanceMetadataDefaults",
        "ec2:AllocateIpamPoolCidr",
        "ec2:ModifySnapshotAttribute",
        "ec2:StartInstances",
        "ec2:CreateNetworkInterfacePermission",
        "ec2:DisassociateRouteServer",
        "ec2:CreateRoute",
        "ec2:CreateInternetGateway",
        "ec2:CreateCoipPoolPermission",
        "ec2:DeleteInternetGateway",
        "ec2:ModifyRouteServer",
        "iam:ListRolePolicies",
        "cloudformation:ListStackResources",
        "states:*",
        "iam:GetRole",
        "ec2:ModifyAddressAttribute",
        "iam:GetPolicy",
        "ec2:ImportKeyPair",
        "ec2:CreateTags",
        "ec2:ModifyNetworkInterfaceAttribute",
        "ec2:RunInstances",
        "ec2:ModifySecurityGroupRules",
        "ec2:AssignPrivateIpAddresses",
        "ec2:DisassociateRouteTable",
        "ec2:StopInstances",
        "ec2:CreateVolume",
        "ec2:CreateNetworkInterface",
        "ec2:AssociateSecurityGroupVpc",
        "ec2:CreateDefaultVpc",
        "ec2:DisassociateIamInstanceProfile",
        "ec2:EnableImageDeprecation",
        "ec2:CopyVolumes",
        "ec2:ModifyInstanceMaintenanceOptions",
        "ec2:CreateSubnet",
        "ec2:CreateLocalGatewayRouteTablePermission",
        "iam:GetRolePolicy",
        "ec2:AttachVolume",
        "ec2:DisassociateAddress",
        "ec2:ModifyVpcEndpointServicePermissions",
        "ec2:ImportVolume",
        "ec2:MoveAddressToVpc",
        "ec2:AttachApplianceToNatGateway",
        "ec2:CancelImageLaunchPermission",
        "ec2:CreateVpc",
        "ec2:ReleaseHosts",
        "ec2:EnableFastSnapshotRestores",
        "ec2:CreateDefaultSubnet",
        "ec2:DeleteLaunchTemplateVersions",
        "ec2:ModifyInstanceEventWindow",
        "ec2:ReleaseAddress",
        "ec2:CreateOdbNetworkPeering",
        "ec2:RebootInstances",
        "ec2:EnableImage",
        "ec2:Describe*",
        "ec2:ModifyInstanceMetadataOptions",
        "ec2:DeleteLaunchTemplate",
        "ec2:AssignIpv6Addresses",
        "ec2:CreateLocalGatewayRoute",
        "ec2:ResetSnapshotAttribute",
        "ec2:ImportSnapshot",
        "iam:ListRoles",
        "ec2:DisassociateSubnetCidrBlock",
        "iam:CreatePolicy",
        "ec2:RestoreAddressToClassic",
        "ec2:CreateLaunchTemplate",
        "kms:ListAliases",
        "ec2:AllocateHosts",
        "ec2:ModifyOdbNetworkPeering",
        "ec2:UnlockSnapshot",
        "ec2:EnableSnapshotBlockPublicAccess",
        "ec2:DeleteInstanceEventWindow",
        "ec2:ConfirmProductInstance",
