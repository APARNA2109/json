{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "DeploymentID": {
            "type": "string",
            "metadata": {
                "description": "Unique identifier for the deployment"
            }
        },
        "DeploymentRegion": {
            "type": "string",
            "defaultValue": "centralindia"
        },
        "WinServerLabVM01Username": {
            "type": "string",
            "metadata": {
                "description": "Admin username for Windows Server VMs"
            },
            "defaultValue": "labuser01"
        },
        "WinServerLabVM01Password": {
            "type": "securestring",
            "metadata": {
                "description": "Admin password for Windows Server VMs"
            },
            "defaultValue": "Password.1!!"
        },
        "WinServerLabVM02Username": {
            "type": "string",
            "metadata": {
                "description": "Admin username for Windows Server VMs"
            },
            "defaultValue": "labuser02"
        },
        "WinServerLabVM02Password": {
            "type": "securestring",
            "metadata": {
                "description": "Admin password for Windows Server VMs"
            },
            "defaultValue": "Password.2!!"
        },
        "KaliLabVMUsername": {
            "type": "string",
            "metadata": {
                "description": "Admin username for Windows Server VMs"
            },
            "defaultValue": "kaliuser"
        },
        "KaliLabVMPassword": {
            "type": "securestring",
            "metadata": {
                "description": "Admin password for Windows Server VMs"
            },
            "defaultValue": "Password.3!!"
        },
        "MetasploitLabVMUsername": {
            "type": "string",
            "metadata": {
                "description": "Admin username for Windows Server VMs"
            },
            "defaultValue": "msfadmin"
        },
        "MetasploitLabVMPassword": {
            "type": "securestring",
            "metadata": {
                "description": "Admin password for Windows Server VMs"
            },
            "defaultValue": "msfadmin"
        },
        "trainerUserName": {
            "type": "string",
            "defaultValue": "traineruser"
        },
        "trainerUserPassword": {
            "type": "securestring",
            "defaultValue": "Password.1!!"
        }
